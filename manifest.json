{
   "manifest_version": 2,
   "name": "CPN Webdash",
   "description": "Quick launcher for CPN websites",
   "version": "1.0.0",
   "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
         "16": "icons/icon16.png",
         "32": "icons/icon32.png"
      }
   },
   "permissions": ["activeTab"]
}